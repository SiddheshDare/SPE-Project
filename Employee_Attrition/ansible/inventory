[web_servers]
web-server ansible_host=<web_server_ip> ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/id_rsa

[k8s_master]
localhost ansible_connection=local ansible_python_interpreter=/usr/bin/python3

[k8s_nodes]
k8s-node1 ansible_host=<k8s_node1_ip> ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/id_rsa
k8s-node2 ansible_host=<k8s_node2_ip> ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/id_rsa

[k8s_cluster:children]
k8s_master
k8s_nodes

[local]
localhost ansible_connection=local
